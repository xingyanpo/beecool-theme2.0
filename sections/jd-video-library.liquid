    "name": "Video library",
    "settings": [
    {
        "type": "richtext",
        "id": "title",
        "label": "title"
    }
    ],
    "blocks": [
    {
        "type": "Video-library",
        "name": "Video library",
        "settings": [
            {
                "type": "image_picker",
                "id": "img",
                "label": "Img"
            },
           {
            "type": "url",
            "id": "link",
            "label": "link"
           
           }
    
          
  ]
}
    ],
    "presets": [{
    "name": "Video library"
    
    }]
}
{% endschema %}