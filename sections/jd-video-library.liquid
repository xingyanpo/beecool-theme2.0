{{ 'jd_video_library_all.css' | asset_url | stylesheet_tag }}
<div class="jd_video_library_all">
    <div class="container">
        <div class="jd_video_library_title ebike_h1">
            {{section.settings.title}}
        </div>
        <div class="jd_video_library_bottom">
            <div class="swiper VideolibrarySwiper">
                <div class="swiper-wrapper">
                    {% for block in section.blocks %}
                        <div class="swiper-slide">
                            <div class="VideolibrarySwiper_box">
                                <div class="Hideimg4">
                                    <img loading="lazy" {% render 'image-attributes' , image: block.settings.img%}>
                                </div>
                                <div class="VideolibrarySwiper_url">{{block.settings.link}}</div>
