{{ 'jd-Instagrams.css' | asset_url | stylesheet_tag }}
<div class="jd-Instagram-all">
    <div class="Instagram-list-content">
        {% if section.settings.title %}
            <div class="text-desc">
                <div class="item-text">{{section.settings.title}}</div>
                <div class="item-text_one">{{section.settings.title_one}}</div>
            </div>
        {% endif %}
        <div class="swiper instagramSwiper">
            <div class="swiper-wrapper">
                
                {%- for block in section.blocks -%}
                    <div class="swiper-slide">
                        <a href="{{block.settings.link}}" target="_blank">
