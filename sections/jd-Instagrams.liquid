  <script>
      var swiper = new Swiper(".instagramSwiper", {
        breakpoints: { 
            280: {  //当屏幕宽度大于等于320
                direction: 'horizontal',
                loop: true,
                autoplay: 5000,
                slidesPerView: "auto",
                centeredSlides:true,
                spaceBetween: 9,
                
               
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
            },
            768: {  //当屏幕宽度大于等于768 
                slidesPerView: 3,
                spaceBetween: 20,
                // navigation: {
                //     nextEl: '.swiper-button-next',
                //     prevEl: '.swiper-button-prev',
                // }
            },
            1300: {  //当屏幕宽度大于等于768 
                slidesPerView: 5,
                spaceBetween: 20,
                // navigation: {
                //     nextEl: '.swiper-button-next',
                //     prevEl: '.swiper-button-prev',
                // }
            }
           
        }
      });
  </script>
  
